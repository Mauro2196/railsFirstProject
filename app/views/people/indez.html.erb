<h1>People#indez</h1>
<p>Find me in app/views/people/indez.html.erb</p>

<table class="table">
  <thead>
    <tr>
      <th>id</th>
      <th>name</th>
      <th>email</th>
      <th>age</th>
      <th>gender</th>
    </tr>
  </thead>
  <tbody>
    <% @people.each do |person| %>
      <tr>
        <td><%= person.id %></td>
        <td><%= person.name %></td>
        <td><%= person.email %></td>
        <td><%= person.edad %></td>
        <td><%= person.genero == true ? "Male" : "Female" %></td>
      </tr>
    <%end%>
  </tbody>
</table>